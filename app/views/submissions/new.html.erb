<h1>New submission</h1>

<%= form_for @submission do |f| %>
<div class="form-group">
  <%= f.label :round %>
  <%= f.text_field :round, class: 'form-control'  %>
</div>

<div class="form-group">
  <%= f.label :submission %>
  <%= f.text_area :submission, class: 'form-control'  %>
</div>

<div class="form-group">
  <%= f.submit class: 'btn btn-primary' %>
</div>

<% end %>
