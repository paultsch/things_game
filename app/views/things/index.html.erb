<h1>Things index table</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Things...</th>
    </tr>
  </thead>
  <tbody>
    <% @things.each do |thing| %>
      <tr>
        <td><%= thing.id %></td>
        <td><%= thing.thing %></td>
      </tr>
    <% end %>
  </tbody>
</table>
